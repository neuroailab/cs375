   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "#load from tfrecords\n",
    "import cPickle\n",
    "data_path = '/home/mrowca/neural_data.pkl' # CHANGE THIS TO '/datasets/neural_data/neural_data.pkl'\n",
    "with open(data_path) as f:\n",
    "    data = cPickle.load(f)\n",
    "meta = data['meta']\n",
    "IT_features = data['IT']"
   ]
